@{
    ViewBag.Title = "Bajas de personal";
    Layout = "~/Views/Shared/LayoutPageExt.cshtml";

    var usuarioConsultaTeorico = (bool)ViewBag.UsuarioDeConsultaTeorico;
    var usuarioConsulta = (bool)ViewBag.UsuarioDeConsulta;
    var permisoAccionEditarDiasBajas = (bool)ViewBag.UsuarioPermisoEditarDiasBajas;
    var usuarioNotificar = (bool)ViewBag.UsuarioNotificar;

    var fechaMinima = (string)ViewBag.fechaMinima;
    var fechaMaxima = (string)ViewBag.fechaMaxima;
    var empresaActual = (int)ViewBag.empresaActual;
    var usuarioLiberarNominas = (bool)ViewBag.usuarioLiberarNominas;
}

<!-- CSS -->
<link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet" />
<link href="~/Content/style/datatables/dataTables.jqueryui.css" rel="stylesheet" />
<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">

<!-- <script src="~/Scripts/bootstrap-toggle.min.js"></script> -->
<link href='~/Content/style/datatables/datatables.min.css' rel='stylesheet' />
<link href="~/Content/style/datatables/fixedColumns.bootstrap.css" rel="stylesheet" />
<link href="~/Content/style/datatables/fixedHeader.bootstrap.css" rel="stylesheet" />
<link href='~/Content/select2.css' rel='stylesheet' />
<link href="~/Content/bootstrap-toggle.min.css" rel="stylesheet" />
<link href='~/Content/style/css/RecursosHumanos/BajasPersonal/BajasPersonal.css' rel='stylesheet' />
<link href='~/Content/style/css/Style-Reportes.css' rel='stylesheet' />
<link type="text/css" href="//gyrocode.github.io/jquery-datatables-checkboxes/1.2.12/css/dataTables.checkboxes.css" rel="stylesheet" />

<style>
    #btnCEBajaPersonal {
        @(usuarioConsulta ? "display: none !important;" : "")
    }
    .verTeorico {
        @(usuarioConsultaTeorico ? "display: none;" : "")
    }
</style>

@if (empresaActual == 1)
{
    <input type="text" id="empresaActual" value="1" style="display: none;">
}
else if(empresaActual == 2)
{
    <input type="text" id="empresaActual" value="2" style="display: none;">
}
else if(empresaActual == 3)
{
    <input type="text" id="empresaActual" value="3" style="display: none;">
}
else if(empresaActual == 6)
{
    <input type="text" id="empresaActual" value="6" style="display: none;">
}


<!-- PERMISOS -->
@if (usuarioConsultaTeorico) {
    <input id="inputPermisoUsuarioTeorico" style="display: none;" value="1">;
}else{
    <input id="inputPermisoUsuarioTeorico" style="display: none;" value="0">;
}

@if(usuarioNotificar)
{
    <input id="inputPermisoUsuarioNotificar" style="display: none;" value="1">;

}else{
    <input id="inputPermisoUsuarioNotificar" style="display: none;" value="0">;

}

@if (usuarioLiberarNominas) {
    <input id="inputPermisoUsuarioLiberarNominas" style="display: none;" value="1">;
}else{
    <input id="inputPermisoUsuarioLiberarNominas" style="display: none;" value="0">;
}
<!-- END: PERMISOS -->

<div class="row">
    <div class="col-sm-12">
        <h3 class="mb5 mt5">BAJAS DE PERSONAL</h3>
    </div>
</div>

<div class="contenedor">
    <div class="row">
        <div class="col-sm-6">
            <div class="form-group">
                <label>CC</label>
                <select class="form-control" id="selectCC" multiple></select>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <label>Estatus</label>
                <select class="form-control select2" id="cboFiltroEstatus">
                    <option value="">--Seleccione--</option>
                    <option value="1">Contratables</option>
                    <option value="2">No Contratables</option>
                </select>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <label>Prioridad</label>
                <select class="form-control select2" id="cboFiltroPrioridad">
                    <option value="">--Seleccione--</option>
                    <option value="1">Alta</option>
                    <option value="2">Media</option>
                    <option value="3">Baja</option>
                </select>
            </div>
        </div>

        <div class="col-sm-3">
            <div class="form-group">
                <label>Fecha Inicio</label>
                <input type="date" id="fechaFiltroInicio" name="min" class="form-control text-center" autocomplete="off" placeholder="##/##/####" min="2000-01-01"></td>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <label>Fecha Final</label>
                <input type="date" id="fechaFiltroFin" name="min" class="form-control text-center" autocomplete="off" placeholder="##/##/####" min="2000-01-01"></td>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <label>Fecha Inicio Liberacion</label>
                <input type="date" id="fechaFiltroContaInicio" name="min" class="form-control text-center" autocomplete="off" placeholder="##/##/####" min="2000-01-01"></td>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <label>Fecha Final Liberacion</label>
                <input type="date" id="fechaFiltroContaFin" name="min" class="form-control text-center" autocomplete="off" placeholder="##/##/####" min="2000-01-01"></td>
            </div>
        </div>

        <div class="col-sm-2">
            <div class="form-group">
                <label># Emp.</label>
                <input id="inputClaveEmpleado" class="form-control">
            </div>
        </div>
        <div class="col-sm-4">
            <div class="form-group">
                <label>Nombre</label>
                <input id="inputNombreEmpleado" class="form-control">
            </div>
        </div>
        <div class="col-sm-1" style="text-align: center;">
            <div class="form-group">
                <label>Autorizado</label>
                <button class="btn btn-xs btn-success botonRedondo" style="display:block;margin-left:auto;margin-right:auto;" disabled><i class="fa fa-check"></i></button>
            </div>
        </div>
        <div class="col-sm-1" style="text-align: center;">
            <div class="form-group">
                <label>Pendientes</label>
                <button class="btn btn-xs btn-warning botonRedondo" style="display:block;margin-left:auto;margin-right:auto;" disabled><i class=" fa fa-clock"></i></button>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="form-group">
                <label>Tipo</label>
                <select class="form-control select2" id="cboFiltroAnticipada">
                    <option value="">Todas</option>
                    <option value="0">Normal</option>
                    <!-- <option value="1">Anticipada</option> -->
                    <option value="2">Pendientes de Notificar</option>
                    <option value="3">Sin confirmación de baja IMSS</option>
                </select>
            </div>
        </div>

        <div class="col-sm-1">
            <button type="button" class="btn btn-warning" id="btnDiasBajas" @(permisoAccionEditarDiasBajas ? "" : "style=display:none;")>Periodos</button>
        </div>
        <div class="col-sm-5">
            <button type="button" class="btn btn-primary pull-right" id="btnFiltroNuevo" @(usuarioConsulta ? "style=display:none;" : "style=display:block;")>Nuevo</button>
            <button type="button" class="btn btn-default pull-right" id="btnFiltroBuscar" style="margin-right: 5px;">Buscar</button>
            <button type="button" class="btn btn-warning pull-right" id="btnFiltroNotificar" style="margin-right: 5px;">Notificar</button>
        </div>
    </div>
</div>

<div class="contenedor mt20">
    <div class="row">
        <div class="col-sm-12">
            <table id="tblRH_Baja_Registro" class='display compact' style="width:100%"></table>
        </div>
    </div>
</div>

