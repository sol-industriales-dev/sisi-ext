@{
    Layout = "~/Views/Shared/LayoutPage.cshtml";
}
<link href="~/Content/style/FullCalendarV4/Core/main.css" rel="stylesheet" />
<link href="~/Content/style/datatables/jquery.dataTables.css" rel="stylesheet" />
<link href="~/Content/style/css/RecursosHumanos/Capacitacion/PlanCapacitacion.css" rel="stylesheet" />
<link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/css/select2.min.css" rel="stylesheet" />

<div class="col-sm-12" style="margin-top: 33px;">
    <fieldset class="fieldset-custm">
        <div class="row">
            <div class="col-sm-5 mrgTop">
                <div class="input-group">
                    <span class="input-group-addon">Proyecto</span>
                    <select id="selectCentroCosto" class="form-control"></select>
                </div>
            </div>
            <div class="col-sm-3 mrgTop">
                <div class="input-group">
                    <span class="input-group-addon">Temática</span>
                    <select id="selectTematica" class="form-control" multiple></select>
                </div>
            </div>
            <div class="col-sm-4 mrgTop">
                <button id="botonBuscar" class="btn btn-default" style="background-color:#d9534f"><i class="fas fa-search"></i>Buscar</button>
                <button id="botonImprimir" class="btn btn-default" style="background-color:#d9534f;"><i class="fas fa-search"></i>Imprimir</button>
            </div>
        </div>
    </fieldset>
</div>
<div class="col-sm-2 mrgTop">
    <fieldset class="fieldset-custm">
        <legend class="legend-custm">Clasificaciones</legend>
        <a class="btn btn-block" style="color: white; cursor: auto; background-color: rgb(255, 102, 0);">Técnico Operativo</a>
        <a class="btn btn-block" style="color: white; cursor: auto; background-color: rgb(192, 0, 0);">Protocolo de Fatalidad</a>
        <a class="btn btn-block" style="color: white; cursor: auto; background-color: rgb(0, 176, 80);">Instructivo Operativo</a>
        <a class="btn btn-block" style="color: white; cursor: auto; background-color: rgb(46, 117, 182);">Normativo</a>
        <a class="btn btn-block" style="color: white; cursor: auto; background-color: rgb(0, 176, 240);">Formativo Normativo</a>
    </fieldset>
</div>
<div class="col-sm-10 mrgTop">
    <div id="divCalendario" style="margin-bottom: 50px;"></div>
</div>

<script src="~/Scripts/html2pdf.bundle.min.js"></script>
<script src="~/Scripts/moment-with-locales.js"></script>
<script src="~/Scripts/moment.min.js"></script>
<script src="~/Scripts/Utilities/datatables/datatables.min.js"></script>
<script src="~/Scripts/Utilities/FullCalendarV4/Core/main.js"></script>
<script src="~/Scripts/Utilities/FullCalendarV4/Core/locales-all.js"></script>
<script src="~/Scripts/Utilities/FullCalendarV4/Daygrid/main.js"></script>
<script src="~/Scripts/Utilities/FullCalendarV4/Interaction/main.js"></script>
<script src="~/Scripts/Utilities/FullCalendarV4/Moment/main.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.min.js"></script>
<script src="~/Scripts/js/Administrativo/RecursosHumanos/Capacitacion/PlanCapacitacion.js"></script>