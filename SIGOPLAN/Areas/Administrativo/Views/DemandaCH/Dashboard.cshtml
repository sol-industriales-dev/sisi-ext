<!-- CONTAINER -->
<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12 text-center">
            <br>
            <h2>DASHBOARD</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <!-- PANEL FILTROS -->
            <div id="panelFiltro" class="panel panel-default panel-principal">
                <div class="panel-heading">
                    <h4 class="text-center">FILTRO DE BÚSQUEDA</h4>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class='col-lg-3'>
                            <label for="cboFiltro_Anio">Año:</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fas fa-user"></i></span>
                                <select id="cboFiltro_Anio" class="form-control select2"></select>
                            </div>
                        </div>
                        <div class='col-lg-4'>
                            <label for="cboFiltro_CC">CC:</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fas fa-user"></i></span>
                                <select id="cboFiltro_CC" class="form-control select2"></select>
                            </div>
                        </div>
                        <div id="divBotonesFiltro" class='pull-right'>
                            <label id="labelBotonBuscar">btnBuscar</label>
                            <button type='button' class='btn btn-default' id='btnFiltroBuscar' title="Buscar registros."><i class='fas fa-search'></i>&nbsp;Buscar</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END: PANEL FILTROS -->
        </div>
    </div>
    <!-- BODY GRAFICAS -->
    <div class="row">
        <!-- COMPORTAMIENTO HISTORICO DE LAS DEMANDAS -->
        <div class="col-lg-6">
            <div id="panelGraficas" class="panel panel-default panel-principal">
                <div class="panel-heading"></div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <div id="graficaComportamientoHistorico"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END: COMPORTAMIENTO HISTORICO DE LAS DEMANDAS -->

        <!-- DEMANDAS ACTIVAS POR AÑO DE PRESENTACIÓN -->
        <div class="col-lg-6">
            <div id="panelGraficas" class="panel panel-default panel-principal">
                <div class="panel-heading"></div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <div id="graficaCantDemandasActivas"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END: DEMANDAS ACTIVAS POR AÑO DE PRESENTACIÓN -->
    </div>
