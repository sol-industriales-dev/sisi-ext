    <div class="row">
        <!-- CANTIDAD DE DEMANDAS SEGÚN SEMAFORO -->
        <div class="col-lg-12">
            <div id="panelGraficas" class="panel panel-default panel-principal">
                <div class="panel-heading"></div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <div id="graficaCantDemandasSegunSemaforo"></div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
