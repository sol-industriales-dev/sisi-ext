@{
ViewBag.Title = "HistoricoFA";
Layout = "~/Views/Shared/LayoutPage.cshtml";
}
<link href="~/Content/style/css/Style-Reportes.css" rel="stylesheet" />
<link href="~/Content/style/datatables/datatables.min.css" rel="stylesheet" />
<link href="~/Content/style/css/Facultamientos/HistoricoFA.css" rel="stylesheet" />
<h1 class="text-center">Histórico de Paquete de Facultamientos</h1>
<fieldset class="fieldset-custm">
    <legend class="legend-custm"><i class="fa fa-history"></i> Paquetes</legend>
    <div class="input-group col-md-3 col-lg-3">
        <span class="input-group-addon">C.C.</span>
        <select id="comboCC" class="form-control"></select>
    </div>
    <div class="table-responsive">
        <table id="tablaPaquetes" class="table table-fixed table-hover table-responsive table-striped">
            <thead class="bg-table-header">
                <tr>
                    <th>C.C.</th>
                    <th>Obra</th>
                    <th>Fecha</th>
                    <th>Estado</th>
                    <th>Versión</th>
                    <th>Agrupador</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</fieldset>
<div id="modalAutorizantes" class="modal fade" role="dialog">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header text-center">
                <h3 class="modal-title"><i class="fa fa-user"></i>
                    Lista de Autorizantes
                </h3>
            </div>
            <div class="modal-body">
                <div class="row container-fluid">
                    <div class="text-center">
                        <h4 id="textoModalDescripcion"></h4>
                        <h4 id="textoModalObra"></h4>
                    </div>
                    <table id="tablaAutorizantes" class="table table-hover table-fixed table-responsive table-striped"
                        width="100%">
                        <thead class="bg-table-header">
                            <tr>
                                <th>Nombre</th>
                                <th>Estatus</th>
                                <th>Tipo</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-arrow-left"></i>
                    regresar</button>
            </div>
        </div>
    </div>
</div>
@Html.Partial("reportViewerView", false)
<script src="~/Scripts/Utilities/datatables/datatables.min.js"></script>
<script src="~/Scripts/js/Administrativo/Facultamiento/HistoricoFA.js"></script>