                            <tr>
                                <th>FECHA INICIO</th>
                                <th>FECHA FIN</th>
                                <th>ID</th>
                                <th>PUESTO</th>
                                <th>DEPTO</th>
                                <th>NÓMINA</th>
                                <th>PLANTILLA ORIGINAL</th>
                                <th>PLANTILLA ACTUAL</th>
                                <th>SUELDO BASE</th>
                                <th>COMPLEMENTO</th>
                                <th>TOTAL NÓMINA</th>
                            </tr>
                        </thead>
