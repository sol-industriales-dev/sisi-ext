<div class="row">
    <h2 class="text-center">Consulta Plantilla Personal</h2>
</div>
<div class="row form-group">
    <div class="col-lg-12">
        <fieldset class="fieldset-custm fielset-Filtros">
            <legend class="legend-custm">Datos:</legend>
            <div class="col-md-3">
                <label>Centro Costos:</label>
                <select class="form-control no-padding" id="cboCC"></select>
            </div>
            <div class="col-md-3">
                <label>Fecha Inicio:</label>
                <input type="text" class="form-control no-padding" id="txtFechaInicio" disabled/>
            </div>
            <div class="col-md-3">
                <label>Fecha Fin:</label>
                <input type="text" class="form-control no-padding" id="txtFechaFin" disabled/>
            </div>
            <div class="col-md-1">
                <button type="button" class="btn btn-info margin-top" id="btnBuscar">
                    <span class="glyphicon glyphicon-search"></span> Cargar
                </button>
            </div>
            <div class="col-md-1">
                <button type="button" class="btn btn-info margin-top" id="btnExportar">
                    <span class="glyphicon glyphicon-search"></span> Exportar
                </button>
            </div>
        </fieldset>
    </div>
</div>

<div class="row form-group">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" id="divFilstroBusqueda">
        <fieldset class="fieldset-custm">
            <div class="row">
                <div class="col-md-12">
                    <h3 class="panel-title text-center" style="font-weight: bold;">
                        Plantilla
                    </h3>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <table id="tblData" class="display bg" width="100%">
