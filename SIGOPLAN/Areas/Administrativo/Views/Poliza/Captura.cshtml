@{
    ViewBag.Title = "Captura";
    Layout = "~/Views/Shared/LayoutPage.cshtml";
}
<link href="~/Content/style/css/Contabilidad/Poliza/Captura.css" rel="stylesheet" />
<link href="~/Content/style/datatables/datatables.min.css" rel="stylesheet" />
<div class="container-fluid">
    <h1 class="text-center">Pólizas</h1>
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#tabPoliza">Gestor de Pólizas</a></li>
        @*<li><a data-toggle="tab" href="#tabCadena">Archivos Pendientes</a></li>*@
        <li><a data-toggle="tab" href="#tabAplicado">Cadenas Productivas</a></li>
    </ul>
    <div class="tab-content">
        <div id="tabPoliza" class="tab-pane fade in active">
            <fieldset class="fieldset-custm">
                <legend class="legend-custm">Búsqueda</legend>
                <div class="col-md-2 form-group">
                    <label>Inicio:</label>
                    <input type="text" id="dtIni" class="form-control" />
                </div>
                <div class="col-md-2 form-group">
                    <label>Fin:</label>
                    <input type="text" id="dtFin" class="form-control" />
                </div>
                <div class="col-md-1">
                    <label>Número:</label>
                    <input type="number" id="numPoliza" class="form-control numPoliza" min="0" onkeypress="return event.charCode >= 48 && event.charCode <= 57" />
                </div>
                <div class="col-md-3">
                    <label>Tipo:</label>
                    <select type="text" class="form-control" id="selTp"></select>
                </div>
                <div class="col-md-1">
                    <button id="btnGetPoliza" class="btn btn-primary margin-top">
                        <i class="fa fa-search" aria-hidden="true"></i> Buscar
                    </button>
                </div>
                <div class="col-md-1 hidden">
                    <button id="btnReporte" type="button" class="btn btn-primary margin-top">
                        <i class="fa fa-print" aria-hidden="true"></i> Reporte
                    </button>
                </div>
                <div class="col-md-1">
                    <button id="btnNuevo" type="button" class="btn btn-primary margin-top">
                        <i class="fa fa-plus" aria-hidden="true"></i> Póliza
                    </button>
                </div>
            </fieldset>
            <fieldset class="fieldset-custm">
                <legend class="legend-custm">Resultados</legend>
                <div class="row">
                    <div class="col-lg-12 container-fluid">
                        <table id="tblPolizas" class="table table-condensed table-hover table-striped text-center">
                            <thead class="bg-table-header">
                                <tr>
                                    <th>PÓLIZA</th>
                                    <th>TIPO PÓLIZA</th>
                                    <th>FECHA</th>
                                    <th>CARGO</th>
                                    <th>ABONO</th>
                                    <th>DIFERENCIA</th>
                                    <th>ESTATUS</th>
                                    <th>GENERÁDA</th>
                                    <th>CONCEPTO</th>
                                    <th></th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </fieldset>
        </div>
        <div id="tabCadena" class="tab-pane fade">
            <fieldset class="fieldset-custm">
                <legend class="legend-custm">Acciones</legend>
                <div class="col-md-2 col-lg-2">
                    <label>Moneda:</label>
                    <select type="text" class="form-control" id="selCadMoneda">
                        <option value="1">Pesos</option>
                        <option value="2">Dólares</option>
                    </select>
                </div>
            </fieldset>
            <fieldset class="fieldset-custm">
                <legend class="legend-custm">Pólizas Factoraje</legend>
                <table id="tblCadena" class="table table-condensed table-hover table-striped text-center" width="100%">
                    <thead class="bg-table-header">
                        <tr>
                            <th># PROVEEDOR</th>
                            <th>PROVEEDOR</th>
                            <th>MONTO</th>
                            <th>FECHA EMISION</th>
                            <th>FECHA VENCIMIENTO</th>
                            <th>IMPRIMIR</th>
                            <th>PAGAR</th>
                        </tr>
                    </thead>
                </table>
            </fieldset>
        </div>
        <div id="tabAplicado" class="tab-pane fade">
            <fieldset class="fieldset-custm">
                <legend class="legend-custm">Acciones</legend>
                <div class="col-md-3 col-lg-3">
                    <div class="input-group">
                        <span class="input-group-addon" id="basic-addon3">Moneda</span>
                        <select type="text" class="form-control" id="selAplMoneda">
                            <option value="1">Pesos</option>
                            <option value="2">Dólares</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-1 col-lg-1">
                    <button id="btnLstAplicado" type="button" class="btn btn-success">
                        <i class="fa fa-table" aria-hidden="true"></i> Diarias
                    </button>
                </div>
                <div class="col-md-1 col-lg-1">
                    <button id="btnLstLimpiar" type="button" class="btn btn-default">
                        <i class="fa fa-eraser" aria-hidden="true"></i> Limpiar
                    </button>
                </div>
            </fieldset>
            <fieldset class="fieldset-custm">
                <legend class="legend-custm">Resultados</legend>
                <div class="table-responsive">
                    <table id="tblAplicado" class="table table-condensed table-hover table-striped text-center" width="100%">
                        <thead class="bg-table-header">
                            <tr>
                                <th># PROVEEDOR</th>
                                <th>PROVEEDOR</th>
                                <th>MONTO</th>
                                <th>FECHA EMISION</th>
                                <th>FECHA VENCIMIENTO</th>
                                <th>IMPRIMIR</th>
                                <th>PAGAR</th>
                                <th></th>
                            </tr>
                        </thead>
                    </table>
                </div>                
            </fieldset>
        </div>
    </div>
</div>
<div id="modalCaptura" class="modal fade" role="dialog" data-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header text-center">
                <h4 class="modal-title">
                    <i class="fa fa-archive"></i>
                    Captura
                </h4>
            </div>
            <div class="modal-body scrollbox">
                <div class="row">
                    <div class="col-xs-2 col-sm-6 col-md-3 col-lg-3">
                        <div class="input-group">
                            <span class="input-group-addon" id="basic-addon3">Tipo Póliza</span>
                            <select type="text" class="form-control" id="selCapTp"></select>
                        </div>
                    </div>
                    <div class="col-xs-2 col-sm-6 col-md-3 col-lg-3">
                        <div class="input-group">
                            <span class="input-group-addon" id="basic-addon3">Estatus</span>
                            <select type="text" class="form-control" id="txtCapStatus">
                                <option value="A">Actualizado</option>
                                <option value="B">Bloqueada</option>
                                <option value="C">Capturada</option>
                                <option value="E">Errónea</option>
                                <option value="V">Validada</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-2 col-sm-6 col-md-3 col-lg-3">
