                            <div class="col-xs-2 col-sm-6 col-md-4 col-lg-4">
                                <div class="input-group">
                                    <span class="input-group-addon" id="basic-addon3">Cuenta</span>
                                    <input type="text" class="form-control" id="txtCapCuenta" readonly />
                                </div>
                            </div>
                            <div class="col-xs-2 col-sm-6 col-md-4 col-lg-4">
                                <div class="input-group">
                                    <span class="input-group-addon" id="basic-addon3">Interfase</span>
                                    <input type="text" class="form-control" id="txtCapITipo" style="width: 50px; display: inline-block;" readonly />
                                    <input type="text" class="form-control" id="txtCapIDesc" style="width: 255px; display: inline-block;" readonly />
                                </div>
                            </div>
                            <div class="col-xs-4 col-sm-4">
                                <div class="input-group">
                                    <span class="input-group-addon" id="basic-addon3">T.M.</span>
                                    <input type="text" class="form-control" id="txtCapTm" readonly />
                                </div>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-lg-12 margin-top">
                        <table id="tblMovPolizas" class="table table-condensed table-hover text-center" width="100%">
                            <thead class="bg-table-header">
                                <tr>
                                    <th>No</th>
                                    <th>Cta</th>
                                    <th>SCta</th>
                                    <th>SSCta</th>
                                    <th>D</th>
                                    <th>Mov</th>
                                    <th>Proveedor</th>
                                    <th>Referencia</th>
                                    <th>CC</th>
                                    <th>O.C./Cte</th>
                                    <th>AC</th>
                                    <th>Concepto</th>
                                    <th>Tipo Movimiento</th>
                                    <th>Monto</th>
                                </tr>
                            </thead>
                        </table>
                        <label id="msjError"></label>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="col-xs-2 col-sm-6 col-md-3 col-lg-3">
                    <div class="input-group">
                        <span class="input-group-addon" id="basic-addon3">Precio del dolar</span>
                        <input type="text" class="form-control" id="txtDolar" readonly />
                    </div>
                </div>
                <button type="button" id="btnGuardar" class="btn btn-primary disabled"><i class="fa fa-save" aria-hidden="true"></i> Guardar</button>
                <button type="button" id="btnAddMov" class="btn btn-success"><i class="fa fa-plus" aria-hidden="true"></i> Movimiento</button>
                <button type="button" id="btnCargarIva" class="btn btn-success btnIva" data-aplicado="1"><i class="fa fa-plus" aria-hidden="true"></i> IVA</button>
                <button type="button" id="btnAbonarIva" class="btn btn-success btnIva" data-aplicado="1"><i class="fa fa-minus" aria-hidden="true"></i> IVA</button>
                <button type="button" id="btnDiffCambiaria" class="btn btn-success btnIva"><i class="fa fa-dollar" aria-hidden="true"></i> Dif. Cambiaria</button>
                <button type="button" class="btn btn-warning" data-dismiss="modal"><i class="fa fa-arrow-left" aria-hidden="true"></i> a gestión</button>
                <button type="button" id="btnLimpiar" class="btn btn-default"><i class="fa fa-eraser" aria-hidden="true"></i> Limpiar</button>
                <button type="button" id="btnEliminar" class="btn btn-danger"><i class="fa fa-close" aria-hidden="true"></i> Eliminar</button>
            </div>
        </div>
    </div>
</div>
