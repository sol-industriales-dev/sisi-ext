@{
    ViewBag.Title = "Alta de empleados";
    Layout = "~/Views/Shared/LayoutPage.cshtml";

    var usuarioConsulta = (bool)ViewBag.UsuarioDeConsulta;
    var usuarioConsultaTeorico = (bool)ViewBag.UsuarioDeConsultaTeorico;
    var permisoAccionEditarFechaCambio = (bool)ViewBag.UsuarioPermisoAccionEditarFechaCambio;
    var permisoAccionEditarDiasIngreso = (bool)ViewBag.UsuarioPermisoEditarDiasIngresos;
    var permisoAccionOcultarSalarioByCC = (bool)ViewBag.UsuarioPermisoAccionOcultarSalarioByCC;
    var permisoMostarBotones = (bool)ViewBag.UsuarioPermisoMostarBotones;
    var empresa = (int)ViewBag.Empresa;

    var fechaMinima = (string)ViewBag.fechaMinima;
    var fechaMaxima = (string)ViewBag.fechaMaxima;

    var permisoTabuladorAltaEmpleado = (bool)ViewBag.PermisoTabuladorAltaEmpleado;

    var puedeAutorizarMasivo = (bool)ViewBag.AutorizacionMasivaActiva;
    var empleadosAdmn = (int)ViewBag.empleadosAdmn;

    var usuarioVerSalarios = (bool)ViewBag.UsuarioVerSalarios;
}

<!-- CSS -->
<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
<link href="~/Content/style/Sytle-Wizard.css" rel="stylesheet" />
<link href='~/Content/style/datatables/datatables.min.css' rel='stylesheet' />
<link href='~/Content/select2.css' rel='stylesheet' />
<link href="~/Content/style/css/RecursosHumanos/Reclutamientos/AltaEmpleados.css" rel="stylesheet" />
<link href="~/Content/style/css/Style-Reportes.css" rel="stylesheet" />
<link type="text/css" href="//gyrocode.github.io/jquery-datatables-checkboxes/1.2.12/css/dataTables.checkboxes.css" rel="stylesheet" />
<link href="~/Content/style/fontawesome-free-6.3.0-web/css/all.css" rel="stylesheet" />

<style>
    .actualizarFamiliar {
    @(usuarioConsulta ? "display: none;" : "")
    }
    .eliminarFamiliar  {
    @(usuarioConsulta ? "display: none;" : "")
    }
    .actualizarUniforme {
    @(usuarioConsulta ? "display: none;" : "")
    }
    .eliminarUniforme  {
    @(usuarioConsulta ? "display: none;" : "")
    }
    .cambiarContratable {
    @(usuarioConsulta ? "display: none;" : "")
    }
    .reingresoEmpleado {
    @(usuarioConsulta ? "display: none;" : "")
    }
    .btnEliminarContrato {
    @(usuarioConsulta ? "display: none;" : "")
    }

    .verTeorico {
    @(usuarioConsultaTeorico ? "display: none;" : "")
    }

    #mdlCrearEditarEmpleadoEK .nav-tabs i {
        position: relative;
    }

    #mdlCrearEditarEmpleadoEK .nav-tabs a {
        padding: 7px 5px;
    }
</style>

<!-- PERMISOS -->

@if (permisoAccionEditarFechaCambio)
{
                                <input id="inputPermisoEditarFechaCambio" style="display: none;" value="1">
    ;
}
else
{
                                <input id="inputPermisoEditarFechaCambio" style="display: none;" value="0">
    ;
}

@if (permisoAccionOcultarSalarioByCC)
{
                                <input id="inputPermisoOcultarTabulador" style="display: none;" value="1">
    ;
}
else
{
                                <input id="inputPermisoOcultarTabulador" style="display: none;" value="0">
    ;
}

@if (empresa == 6)
{
                                <input id="txtEmpresa" style="display: none;" value="6">
    ;
}
else if (empresa == 3)
{
                                <input id="txtEmpresa" style="display: none;" value="3">
    ;
}

@if (empleadosAdmn == 1)
{
    <input id="empleadosAdmn" style="display: none;" value="1">

}else{
    <input id="empleadosAdmn" style="display: none;" value="0">

}

@if(permisoMostarBotones)
{
    <input id="inputPermisoMostarBotones" style="display: none;" value="1">

}else{
    <input id="inputPermisoMostarBotones" style="display: none;" value="0">

}

@if (usuarioVerSalarios)
{
    <input id="inputPermisoVerSalarios" style="display: none;" value="1">
    ;
}
else
{
    <input id="inputPermisoVerSalarios" style="display: none;" value="0">
    ;
}
<!-- END PERMISOS -->

<select id="motivoSueldo" style="display:none;"></select>

<!-- CONTAINER FLUID -->
<div class="container-fluid">
    <br><br>
    <!-- PANEL EMPLEADOS -->
    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-default">
                <div class="panel-heading text-center">
                    <h2>EMPLEADOS</h2>
                    <h4>
                        <span id="txtHeadEstatusA">Altas: </span><span style="color: green;" id="txtHeadCantA">0</span>
                        <span id="txtHeadEstatusB">Bajas: </span><span style="color: red;" id="txtHeadCantB">0</span>
                        <span id="txtHeadEstatusP">Pendientes: </span><span style="color: #fd7e14;" id="txtHeadCantP">0</span>
                        <span id="txtHeadEstatusC">Canceladas: </span><span class="text-muted" id="txtHeadCantC">0</span>
                    <h4>
                </div>

                <!-- FILTROS -->
                <div class="panel-body">
                    <div class="row">
                        <div class="col-sm-5">
                            <div class="input-group">
                                <span class="input-group-addon">CC</span>
                                <select class="form-control" id="cboFiltroCC" multiple></select>
                            </div>
                        </div>
                        <div class="col-sm-3" >
                            <div class="input-group">
                                <span class="input-group-addon">Estatus</span>
                                <select style="width: 100%;" name="" id="chkFiltroEsPendiente" class="form-control">
                                    <option value="">-Seleccione--</option>
                                    <option value="A">Activo</option>
                                    <option value="B">Baja</option>
                                    <option value="P">Pendiente</option>
                                    <option value="C">Cancelado</option>
                                </select>
                            </div>
                        </div>
                        <div class="pull-right">
                            <button type="button" class="btn btn-info" id="btnEnviarCorreos">Notificar</button>
                            <button type="button" class="btn btn-info" id="btnAutorizacionMultiple" @(usuarioConsulta || !puedeAutorizarMasivo ? "style=display:none;" : "") disabled>Autorizar</button>
                            <button type="button" class="btn btn-warning" id="btnDiasIngresos" @(permisoAccionEditarDiasIngreso ? "" : "style=display:none;")>Periodos</button>
                            <button type="button" class="btn btn-primary" id="btnFiltroExportar">Exportar</button>
                            <button type="button" class="btn btn-primary" id="btnFiltroBuscar">Buscar</button>
                            <button type="button" class="btn btn-success" id="btnCrearEmpleado">Nuevo</button>
                        </div>
                    </div>
                </div>
                <!-- END: FILTROS -->

                <!-- LISTADO DE EMPLEADOS -->
                <div class="row">
                    <div class="col-sm-12" id="divTblEmpleados">
                        <table id="tblRH_REC_Empleados" class="table table-hover table-sm table-bordered compact" style="width:100%;"></table>
                    </div>
                </div>
                <!-- END: LISTADO DE EMPLEADOS -->
            </div>
        </div>
    </div>
    <!-- END: PANEL EMPLEADOS -->
</div>
<!-- END: CONTAINER FLUID -->

<!-- MODAL CREAR/EDITAR EMPLEADO EK -->
<div id='mdlCrearEditarEmpleadoEK' class='modal fade' data-backdrop="static" role='dialog' tabindex="0">
    <div class='modal-dialog modal-lg'>
        <div class='modal-content'>
            <div class='modal-header text-center' stlye="padding-bottom: 0 !important;">
                <div class="row">
                    <div class="col-sm-3">
                    </div>
                    <div class="col-sm-6">
                        <h1 class='modal-title' style="bottom: 0px;"><span id="lblTitleCrearEditarEmpleadoEK"></span></h1>
                    </div>
                    <div class="col-sm-3">
                        <div class="pull-right">
                            <a href="#" class="thumbnail">
                                <img style="width:100px;" id="imgFotoEmpleadoHead" src="" alt="...">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class='modal-body'>
                <!-- LISTADO DE CANDIDATOS APROBADOS -->
                <div class="row">
                    <div id="divCandidatosAprobados">
                        <div class="col-lg-7">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">Candidatos aprobados</span>
                                <select class="form-control" id="cboCandidatosAprobados"></select><br>
                            </div>
                        </div>
                    </div>
                    <div class="pull-right">
                        <button id="btnMdlFotoEmpleado" class="btn btn-primary" @(usuarioConsulta ? "style=display:none;" : "style=display:block;")><i class="fas fa-image"></i>&nbsp;Foto del empleado</button>
                    </div>
                </div>
                <!-- END: LISTADO DE CANDIDATOS APROBADOS -->

                <!-- DATOS PRINCIPALES EMPLEADO -->
                <fieldset class='fieldset-custm'>
                    <legend class='legend-custm'></legend>
                    <div class='row'>
                        <br>
                        <div class="col-sm-2">
                            <label tabindex="0" for="lblCEDatosEmpleadoClaveEmpleado">Empleado:&nbsp;<span id="lblCEDatosEmpleadoClaveEmpleado"></span></label>
                        </div>
                        <div class="col-sm-2">
                            <label tabindex="0" for="lblCEDatosEmpleadoEstatus">Estatus:&nbsp;<span id="lblCEDatosEmpleadoEstatus">PENDIENTE</span></label>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">*Fecha Ingreso Actual:</span>
                                <input tabindex="0" type="text" id="txtCEDatosEmpleadoFechaIngreso" class="form-control" min=@fechaMinima max=@fechaMaxima>
                            </div>
                        </div>
                        <div class="col-sm-2" style="display: none;">
                            <button class="btn btn-primary">Credencial</button>
                        </div>
                    </div>
                </fieldset>
                <!-- DATOS PRINCIPALES EMPLEADO -->
                <fieldset class='fieldset-custm'>
                    <legend class='legend-custm'>Datos empleado</legend>
                    <div class="row">

                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">*Apellido paterno:</span>
                                <input type="text" id="txtCEDatosEmpleadoApePaterno" class="form-control" tabindex="0">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">Apellido materno:</span>
                                <input type="text" id="txtCEDatosEmpleadoApeMaterno" class="form-control" tabindex="0">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">*Nombre (s):</span>
                                <input type="text" id="txtCEDatosEmpleadoNombre" class="form-control" tabindex="0">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group" style="display: none;">
                                <span class="input-group-addon">Solicitud:</span>
                                <input type="text" class="form-control" tabindex="0">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon" id="lblCEDatosEmpleadoPaisNac">*País nacimiento:</span>
                                <select class="form-control" data-select2-id="cboCEDatosEmpleadoPaisNacTabIndex" id="cboCEDatosEmpleadoPaisNac" tabindex="0"></select>
                            </div>
                        </div>
                        <div id="divCeDatosEmpleadosDepartamentoNac" class="col-sm-4" @(empresa != 6 ? "style=display:none;" : "style=display:block;")>
                            <div class="input-group marginBottom">
                                <span class="input-group-addon" id="lblCEDatosEmpleadoDepartamentoNac">*Departamento:</span>
                                <select class="form-control comboChange" data-select2-id="cboCEDatosEmpleadoDepartamentoNacIndex" id="cboCEDatosEmpleadoDepartamentoNac" tabindex="0"></select>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">@(empresa == 6 ? "*Provincia" : empresa == 3 ? "*Departamento Nacimiento" : "*Estado Nacimiento"):</span>
                                <select class="form-control" id="cboCEDatosEmpleadoEstadoNac" tabindex="0"></select>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">@(empresa == 6 ? "*Distrito" : "*Lugar nacimiento"):</span>
                                <select class="form-control" id="cboCEDatosEmpleadoLugarNac" tabindex="0"></select>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">*Fecha nacimiento:</span>
                                <input type="text" id="txtCEDatosEmpleadoFechaNacimiento" class="form-control">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">*Localidad nacimiento:</span>
                                <input type="text" id="txtCEDatosEmpleadoLocalidadNac" class="form-control">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group marginBottom">
                                <span class="input-group-addon">*Sexo:</span>
                                <select class="form-control" id="cboCEDatosEmpleadoSexo">
                                    <option value="">-Seleccione--</option>
                                    <option value="F">Femenino</option>
                                    <option value="M">Masculino</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    @if (empresa == 6)
                    {

                                                <div class="row">
                                                    <div class="col-sm-4">
                                                        <div class="input-group marginBottom">
                                                            <span class="input-group-addon">*C.U.S.P.P.:</span>
                                                            <input type="text" id="txtCEDatosEmpleadoCUSPP" class="form-control">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row" @(empresa == 6 || empresa == 3 ? "style=display:none;" : "style=display:block;")>
                                                    <div class="col-sm-4" style="display: none;" >
                                                        <div class="input-group marginBottom">
                                                            <span class="input-group-addon">*C.P. de CIF:</span>
                                                            <input type="text" id="txtCEDatosEmpleadoCPCIF" class="form-control">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class="input-group marginBottom">
                                                            <span class="input-group-addon">*RFC:</span>
                                                            <input type="text" id="txtCEDatosEmpleadoRFC" class="form-control" maxlength="13" >
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class="input-group marginBottom">
                                                            <span class="input-group-addon">*CURP:</span>
                                                            <input type="text" id="txtCEDatosEmpleadoCURP" class="form-control" maxlength="18">
                                                        </div>
                                                    </div>
                                                </div>

                    }
                    else
                    {
                                                <div class="row" @(empresa == 6 || empresa == 3 ? "style=display:none;" : "style=display:block;")>
                                                    <div class="col-sm-4" style="display: none;">
                                                        <div class="input-group marginBottom">
                                                            <span class="input-group-addon">*C.U.S.P.P.:</span>
                                                            <input type="text" id="txtCEDatosEmpleadoCUSPP" class="form-control">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class="input-group marginBottom">
                                                            <span class="input-group-addon">*C.P. de CIF:</span>
                                                            <input type="text" id="txtCEDatosEmpleadoCPCIF" class="form-control">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class="input-group marginBottom">
                                                            <span class="input-group-addon">*RFC:</span>
                                                            <input type="text" id="txtCEDatosEmpleadoRFC" class="form-control" maxlength="13" >
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class="input-group marginBottom">
                                                            <span class="input-group-addon">*CURP:</span>
                                                            <input type="text" id="txtCEDatosEmpleadoCURP" class="form-control" maxlength="18">
                                                        </div>
                                                    </div>
                                                </div>

                    }

                    <div class="row">
                        <div class="col-sm-4" style="display: none;">
                            <button class="btn btn-primary">Seleccionar fotografía</button>
                        </div>
                    </div>
                </fieldset>
                <!-- END: DATOS PRINCIPALES EMPLEADO -->

                <!-- TABS -->
                <ul class="nav nav-tabs" id="myTab" role="tablist" style="height:45px !important; margin-top: 5px;">
                    <li class="nav-item" role="presentation" style="width:120px">
                        <a title="Datos Generales" class="nav-link active" id="home-tab" data-toggle="tab" @(usuarioConsultaTeorico ? "href=#tab" : "href=#tabGeneralesContacto") role="tab" aria-controls="home" aria-selected="true" ><i class="far fa-id-card fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" style="width:120px">
                        <a title="Familiares" class="nav-link" id="profile-tab" data-toggle="tab" @(usuarioConsultaTeorico ? "href=#tab" : "href=#tabFamiliares") role="tab" aria-controls="profile" aria-selected="false"><i class="far fa-user fa-stack-2x fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" style="width:120px">
                        <a title="Beneficiario" class="nav-link" id="contact-tab" data-toggle="tab" @(usuarioConsultaTeorico ? "href=#tab" : "href=#tabBeneficiario") role="tab" aria-controls="contact" aria-selected="false"><i class="fas fa-user-plus fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" style="width:120px">
                        <a title="Compañia" class="nav-link" id="contact-tab" data-toggle="tab" href="#tabCompania" role="tab" aria-controls="contact" aria-selected="false" ><i class="far fa-building fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" @(!permisoTabuladorAltaEmpleado ? "style=display:none;" : "style=width:120px;")>
                        <a title="Nomina" class="nav-link" id="nomina-tab" data-toggle="tab" @(usuarioConsultaTeorico || !usuarioVerSalarios ? "href=#tab" : "href=#tabNomina") role="tab" aria-controls="contact" aria-selected="false"><i class="fas fa-piggy-bank fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" style="width:120px">
                        <a title="Uniforme" class="nav-link" id="contact-tab" data-toggle="tab" href="#tabUniforme" role="tab" aria-controls="contact" aria-selected="false" ><i class="fas fa-tshirt fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" style="width:120px">
                        <a title="Examen medico" class="nav-link" id="contact-tab" data-toggle="tab" @(usuarioConsultaTeorico ? "href=#tab" : "href=#tabExamenMedico") role="tab" aria-controls="contact" aria-selected="false"><i class="fas fa-briefcase-medical fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" style="display:none;">
                        <a title="Maquinaria" class="nav-link" id="contact-tab" data-toggle="tab" @(usuarioConsultaTeorico ? "href=#tab" : "href=#tabMaquinaria") role="tab" aria-controls="contact" aria-selected="false">Maquinaria</a>
                    </li>
                    <li class="nav-item" role="presentation" style="width:120px">
                        <a title="Contratos" class="nav-link" id="contact-tab" data-toggle="tab" @(usuarioConsultaTeorico ? "href=#tab" : "href=#tabContratos") role="tab" aria-controls="contact" aria-selected="false"><i class="far fa-handshake fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" style="width:120px">
                        <a title="Documentos" class="nav-link" id="contact-tab" data-toggle="tab" @(usuarioConsultaTeorico ? "href=#tab" : "href=#tabDocumentos") role="tab" aria-controls="contact" aria-selected="false"><i class="fas fa-archive fa-stack-2x"></i></a>
                    </li>
                    <li class="nav-item" role="presentation" @(empresa != 6 ? "style=display:none;" : "style=width:120px;")>
                        <a title="Info Perú" class="nav-link" id="contact-tab" data-toggle="tab" @(usuarioConsultaTeorico ? "href=#tab" : "href=#tabInfoPeru") role="tab" aria-controls="contact" aria-selected="false"><i class="fa-solid fa-folder-open fa-stack-2x"></i></a>
                    </li>
                </ul>
                <!-- END: TABS -->

                <!-- CONTENIDO TABS -->
                <div class="tab-content" id="myTabContent">
                    <!-- GENERALES Y CONTACTO -->
                    <div class="tab-pane fade" id="tabGeneralesContacto" role="tabGeneralesContacto" aria-labelledby="home-tab">
                        <fieldset class='fieldset-custm'>
                            <legend class='legend-custm'>Generales y contacto</legend>
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="input-group marginBottom">
                                        <span class="input-group-addon">Estado civil</span>
                                        <select id="cboCEGenContactoEstadoCivil" class="form-control"></select>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="input-group marginBottom">
                                        <span class="input-group-addon">Estudios</span>
                                        <input type="text" id="txtCEGenContactoEstudios" class="form-control">
                                    </div>
                                </div>
                                <div class="col-sm-4" style="display:none;">
                                    <div class="input-group marginBottom">
                                        <span class="input-group-addon">Fecha planta</span>
                                        <input type="date" id="txtCEGenContactoFechaPlanta" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="input-group marginBottom">
                                        <span class="input-group-addon">@(empresa == 6 ? "*Provincia" : empresa == 3 ? "*Localidad" : "*Colonia")</span>
                                        <input type="text" id="txtCEGenContactoColonia" class="form-control">
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="input-group marginBottom">
                                        <span class="input-group-addon">Abreviatura</span>
                                        <input type="text" id="txtCEGenContactoAbreviatura" class="form-control">
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="input-group marginBottom">
                                        @if (empresa == 6)
                                        {
                                                                        <span class="input-group-addon" style="display: none;">Cred. elector</span>
                                            ;
                                                                        <input type="text" id="txtCEGenContactoCredElector" class="form-control" style="display: none;">
                                            ;
                                                                        <span class="input-group-addon">DNI</span>
                                            ;
                                                                        <input type="text" id="txtCEGenContactoDNI" class="form-control">
                                            ;
                                        }
