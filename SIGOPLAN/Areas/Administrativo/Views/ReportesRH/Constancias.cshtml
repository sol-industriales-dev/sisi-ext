        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <legend class="modal-title">CONSTANCIA FONACOT</legend>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-6">
                            <label class="hidden">Puesto:</label>
                            <input type="text" class="hidden" id="nombrePuesto" readonly>
                        </div>
                        <div class="col-md-6">
                            <label class="hidden">Nombre del trabajador:</label>
                            <input type="text" class="hidden" id="nombreCompleto" readonly>
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">regPatron:</label>
                            <input type="text" class="hidden" id="regPatron">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">nombrePatron:</label>
                            <input type="text" class="hidden" id="nombrePatron">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden" hidden>imss:</label>
                            <input type="text" class="hidden" id="imss">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">rfc:</label>
                            <input type="text" class="hidden" id="rfc">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">curp:</label>
                            <input type="text" class="hidden" id="curp">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">tipoNomina:</label>
                            <input type="text" class="hidden" id="tipoNomina">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">sueldoBase:</label>
                            <input type="text" class="hidden" id="sueldoBase">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">complemento:</label>
                            <input type="text" class="hidden" id="complemento">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">mensualNeto:</label>
                            <input type="text" class="hidden" id="mensualNeto">
                        </div>
                        <div class="col-md-3">
                            <label class="hidden">fechaIngreso:</label>
                            <input type="text" class="hidden" id="fechaIngreso">
                        </div>
                        <div class="col-md-12">
                            <label class="">ccDescripcionFonacot:</label>
                            <textarea class="form-control" id="ccDescripcionFonacot" rows="3"></textarea>
                        </div>

                    </div>
                </div>
                <br>
            </div>
            <br>
            <div class="modal-footer margin-top">
                <button type="button" id="btnFonacot" class="btn btn-success">imprimir carta</button>
                <button type="button" class="btn btn-default" id="btnCerrar" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>

<!-- #endregion -->

