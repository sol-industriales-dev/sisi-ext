@{
    ViewBag.Title = "Historial clinico";
    Layout = "~/Views/Shared/LayoutPage.cshtml";
    var idEmpresa = new SIGOPLAN.Controllers.Principal.HomeController();
}

<!-- CSS -->
<link href="~/Content/style/datatables/jquery.dataTables.css" rel="stylesheet" />
<link href='~/Content/select2.css' rel='stylesheet' />
<link href="~/Content/style/css/Seguridad/SaludOcupacional/HistorialClinico.css" rel="stylesheet" />
<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
<link href="~/Content/style/css/Style-Reportes.css" rel="stylesheet" />

<!-- CONTAINER FLUID -->
<div class="container-fluid">
    <br><br>
    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-default">
                <div class="panel-heading text-center"><h2>HISTORIAL CLINICO</h2></div>
                 <input class="hidden" id="idEmpresa" value="@idEmpresa.getEmpresaID()" />
                <div class="panel-body">
                    <div class="row">
                        <div class="pull-right">
                            <button type="button" class="btn btn-primary" id="btnFiltroBuscar"><i class="fas fa-search"></i>&nbsp;Buscar</button>
                            <button type="button" class="btn btn-primary marginRight" id="btnShowCEHistorialClinico"><i class="fas fa-plus"></i>&nbsp;Nuevo</button>
                            <button type="button" class="btn btn-primary marginRight" id="btnCECancelar"><i class="fas fa-undo"></i>&nbsp;Regresar</button>
                            <a id="btnGuardarParcial" href="#" class="btn-flotante">Guardar</a>
                        </div><br><br>
                    </div>

                    <div id="divtblS_SO_HistorialesClinicos">
                        <div class="row">
                            <div class="col-xs-12">
                                <table id="tblS_SO_HistorialesClinicos" class="table table-hover table-bordered compact" width="100%"></table>
                            </div>
                        </div>
                    </div>

                    <!-- MODAL CREAR/EDITAR HISTORIAL CLINICO -->
                    <div id="divCEHistorialClinico">

                        <!-- DATOS PERSONALES -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="panel panel-default">
                                    <div class="panel-heading text-center"><h4>DATOS PERSONALES</h4></div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-3">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Folio</span>
                                                    <input type="text" id="txt_dtsPer_Folio" disabled="disabled" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-4"></div>
                                            <div class="col-lg-5">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Imagen de la persona</span>
                                                    <input id="txt_dtsPer_ImagenPersona" type="file" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Empresa</span>
                                                    <select id="cbo_dtsPer_EmpresaID" class="form-control"></select>
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Centro de costo</span>
                                                    <select id="cbo_dtsPer_CCID" class="form-control"></select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Paciente</span>
                                                    <input type="text" id="txt_dtsPer_Paciente" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Fecha y hora</span>
                                                    <input id="txt_dtsPer_FechaHora" type="date" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-3">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Fecha nacimiento</span>
                                                    <input id="txt_dtsPer_FechaNac" type="date" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-1">
                                                <button class="btn btn-primary" id="btnGetEdadPaciente"><i class="fas fa-calculator"></i></button>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Edad</span>
                                                    <input type="text" id="txt_dtsPer_Edad" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Sexo</span>
                                                    <select id="cbo_dtsPer_Sexo" class="form-control">
                                                        <option value="">--Seleccione--</option>
                                                        <option value="M">Masculino</option>
                                                        <option value="F">Femenino</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-4">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Estado civil</span>
                                                    <select id="cbo_dtsPer_EstadoCivil" class="form-control"></select>
                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Tipo sanguineo</span>
                                                    <select id="cbo_dtsPer_TipoSanguineo" class="form-control"></select>
                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">@(idEmpresa.getEmpresaID() == 6 ? "DNI" : "CURP")</span>
                                                    <input type="text" id="txt_dtsPer_CURP" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Domicilio</span>
                                                    <input type="text" id="txt_dtsPer_Domicilio" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Ciudad</span>
                                                    <input type="text" id="txt_dtsPer_Ciudad" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Escolaridad</span>
                                                    <select id="cbo_dtsPer_Escolaridad" class="form-control"></select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Lugar nacimiento</span>
                                                    <input type="text" id="txt_dtsPer_LugarNacimiento" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Teléfono</span>
                                                    <input type="text" id="txt_dtsPer_Telefono" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END: DATOS PERSONALES -->

                        <!-- MOTIVO DE LA EVALUACIÓN -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="panel panel-default">
                                    <div class="panel-heading text-center"><h4>MOTIVO DE LA EVALUACIÓN</h4></div>
                                    <div class="panel-body center">
                                    
                                        <div class="row">
                                            <div class="col-lg-2">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Ingreso?</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_motEva_esIngreso" class="regular-checkbox">
                                                            <label for="chk_motEva_esIngreso" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>

                                            <div class="col-lg-2">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Retiro?</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_motEva_esRetiro" class="regular-checkbox">
                                                            <label for="chk_motEva_esRetiro" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        
                                            <div class="col-lg-3">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Evaluación opcional?</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_motEva_esEvaOpcional" class="regular-checkbox">
                                                            <label for="chk_motEva_esEvaOpcional" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        
                                            <div class="col-lg-3">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Post incapacidad</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_motEva_esPostIncapacidad" class="regular-checkbox">
                                                            <label for="chk_motEva_esPostIncapacidad" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        
                                            <div class="col-lg-2">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Reubicación</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_motEva_esReubicacion" class="regular-checkbox">
                                                            <label for="chk_motEva_esReubicacion" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END: MOTIVO DE LA EVALUACIÓN -->

                        <!-- ANTECEDENTES LABORALES -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="panel panel-default">
                                    <div class="panel-heading text-center"><h4>ANTECEDENTES LABORALES</h4></div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Puesto</span>
                                                    <input id="txt_antLab_Puesto" type="text" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Empresa</span>
                                                    <input id="txt_antLab_Empresa" type="text" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-4">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Desde</span>
                                                    <input id="txt_antLab_Desde" type="text" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Hasta</span>
                                                    <input id="txt_antLab_Hasta" type="text" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Turno</span>
                                                    <input id="txt_antLab_Turno" type="text" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <fieldset class='fieldset-custm'>
                                                <legend class='legend-custm'>Actividades a realizar</legend>

                                                <div class="col-lg-2">
                                                    <fieldset class='fieldset-custm'>
                                                        <div style="display: inline-block;">
                                                            <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿De pie?</label>
                                                            <div style="display: inline-block;">
                                                                <input type="checkbox" id="chk_antLab_esDePie" class="regular-checkbox" checked="checked">
                                                                <label for="chk_antLab_esDePie" style="margin-bottom: 1px;"></label>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>

                                                <div class="col-lg-2">
                                                    <fieldset class='fieldset-custm'>
                                                        <div style="display: inline-block;">
                                                            <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Inclinado?</label>
                                                            <div style="display: inline-block;">
                                                                <input type="checkbox" id="chk_antLab_esInclinado" class="regular-checkbox" checked="checked">
                                                                <label for="chk_antLab_esInclinado" style="margin-bottom: 1px;"></label>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>

                                                <div class="col-lg-2">
                                                    <fieldset class='fieldset-custm'>
                                                        <div style="display: inline-block;">
                                                            <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Sentado?</label>
                                                            <div style="display: inline-block;">
                                                                <input type="checkbox" id="chk_antLab_esSentado" class="regular-checkbox" checked="checked">
                                                                <label for="chk_antLab_esSentado" style="margin-bottom: 1px;"></label>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>

                                                <div class="col-lg-2">
                                                    <fieldset class='fieldset-custm'>
                                                        <div style="display: inline-block;">
                                                            <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Arrodillado?</label>
                                                            <div style="display: inline-block;">
                                                                <input type="checkbox" id="chk_antLab_esArrodillado" class="regular-checkbox" checked="checked">
                                                                <label for="chk_antLab_esArrodillado" style="margin-bottom: 1px;"></label>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>

                                                <div class="col-lg-2">
                                                    <fieldset class='fieldset-custm'>
                                                        <div style="display: inline-block;">
                                                            <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Caminando?</label>
                                                            <div style="display: inline-block;">
                                                                <input type="checkbox" id="chk_antLab_esCaminando" class="regular-checkbox" checked="checked">
                                                                <label for="chk_antLab_esCaminando" style="margin-bottom: 1px;"></label>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                                
                                                <div class="col-lg-2">
                                                    <fieldset class='fieldset-custm'>
                                                        <div style="display: inline-block;">
                                                            <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Otra?</label>
                                                            <div style="display: inline-block;">
                                                                <input type="checkbox" id="chk_antLab_esOtra" class="regular-checkbox" checked="checked">
                                                                <label for="chk_antLab_esOtra" style="margin-bottom: 1px;"></label>
                                                            </div>
                                                        </div>
                                                    </fieldset><br>
                                                </div>

                                                <div class="input-group marginTop">
                                                    <span class="input-group-addon">Cual</span>
                                                    <input id="txt_antLab_Cual" type="text" class="form-control" />
                                                </div>
                                                </fieldset>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END: ANTECEDENTES LABORALES -->

                        <!-- ACCIDENTES Y ENFERMEDADES DE TRABAJO -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="panel panel-default">
                                    <div class="panel-heading text-center"><h4>ACCIDENTES Y ENFERMEDADES DE TRABAJO</h4></div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-5">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Empresa</span>
                                                    <input id="txt_accET_Empresa" type="text" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-2">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Año</span>
                                                    <input id="txt_accET_Anio" type="text" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-5">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Lesión/Área anatómica</span>
                                                    <input id="txt_accET_LesionAreaAnatomica" type="text" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Secuelas</span>
                                                    <input id="txt_accET_Secuelas" type="text" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Cuales</span>
                                                    <input id="txt_accET_Cuales" type="text" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Exam. no aceptables</span>
                                                    <input id="txt_accET_ExamNoAceptables" type="text" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">Causas</span>
                                                    <input id="txt_accET_Causas" type="text" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">¿Abandono de trabajo por problemas de salud o lumbalgias?</span>
                                                    <input id="txt_accET_AbandonoTrabajo type="text"" class="form-control" value="No" />
                                                </div>
                                            </div>

                                            <div class="col-lg-3">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">¿Incapacidad frecuente?</span>
                                                    <input id="txt_accET_IncapacidadFrecuente" type="text" class="form-control" value="No" />
                                                </div>
                                            </div>

                                            <div class="col-lg-3">
                                                <div class="input-group marginBottom">
                                                    <span class="input-group-addon">¿Prolongadas?</span>
                                                    <input id="txt_accET_Prolongadas" type="text" class="form-control" value="No" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END: ACCIDENTES Y ENFERMEDADES DE TRABAJO -->

                        <!-- USO DE ELEMENTOS DE PROTECCIÓN PERSONAL -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="panel panel-default">
                                    <div class="panel-heading text-center"><h4>USO DE ELEMENTOS DE PROTECCIÓN PERSONAL</h4></div>
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-lg-12 marginBottom">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Actual?</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_usoElePP_esActual" class="regular-checkbox" checked="checked">
                                                            <label for="chk_usoElePP_esActual" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-2 marginBottom">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Casco?</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_usoElePP_esCasco" class="regular-checkbox" checked="checked">
                                                            <label for="chk_usoElePP_esCasco" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>

                                            <div class="col-lg-2 marginBottom">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Tapaboca?</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_usoElePP_esTapaboca" class="regular-checkbox" checked="checked">
                                                            <label for="chk_usoElePP_esTapaboca" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>

                                            <div class="col-lg-2 marginBottom">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Gafas?</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_usoElePP_esGafas" class="regular-checkbox" checked="checked">
                                                            <label for="chk_usoElePP_esGafas" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>

                                            <div class="col-lg-2 marginBottom">
                                                <fieldset class='fieldset-custm'>
                                                    <div style="display: inline-block;">
                                                        <label style="font-size: 15px; margin-top: 5px; display: inline-block;">¿Respirador?</label>
                                                        <div style="display: inline-block;">
                                                            <input type="checkbox" id="chk_usoElePP_esRespirador" class="regular-checkbox" checked="checked">
                                                            <label for="chk_usoElePP_esRespirador" style="margin-bottom: 1px;"></label>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        </div>
