@{
    ViewBag.Title = "DashboardSubContratista";
    Layout = "~/Views/Shared/LayoutPage.cshtml";
}

<link href="~/Content/style/css/ControlObra/controlObra2.css" rel="stylesheet" />

<link href='~/Content/style/datatables/datatables.min.css' rel='stylesheet' />
<link href="~/Content/select2.css" rel="stylesheet" />


<style>
    /*TABLAS*/
    table.dataTable {
        width: 100% !important;
    }

        table.dataTable tbody tr td, table thead tr th, table.dataTable, .dataTables_scrollBody {
            border: 0px solid #ffffff !important;
        }

            table.dataTable thead {
                font-size: 15px;
                background-color: #3556ae;
                color: white;
            }

    /*MODALES*/
    .modal-xl {
        width: 80%;
    }

    .disenioModal .modal-header {
        color: white;
        background: linear-gradient(40deg, #45cafc, #303f9f);
        border: none;
    }

        .disenioModal .modal-header .titleHeader {
            font-size: x-large;
            font-weight: bold;
        }

        .disenioModal .modal-header .modal-title {
            font-weight: bold;
        }

        .disenioModal .modal-header .modal-subtitle {
            margin-top: 0px;
            margin-bottom: 0px;
        }
</style>
<script>
    $(document).ready(function () {
        $('#cboProyecto').select2();
    });
    $(document).ready(function () {
        $('#cboSubContratista').select2();
    });
</script>

