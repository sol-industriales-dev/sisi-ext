<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/style/starrr.css")">
<style>
    .btn-huge {
        padding-top: 20px;
        padding-bottom: 20px;
    }

    option[data-prefijo="evaluadoOk"] {
        background-color: #12b400;
    }

    #canvasEstrellasGeneral {
        min-width: 320px;
        max-width: 1300px;
        margin: 1em auto;
    }

    /*HighCharts*/

    #containerHC, #containerHCProv, #containerHCPeores, #containerHCBest{
        height: 400px;
    }

    #containerHCMasEvaluados {
        height: 700px;
    }

    .highcharts-figure, .highcharts-data-table table {
        min-width: 320px;
        max-width: 1920px;
        margin: 1em auto;
    }

    .highcharts-data-table table {
        font-family: Verdana, sans-serif;
        border-collapse: collapse;
        border: 1px solid #EBEBEB;
        margin: 10px auto;
        text-align: center;
        width: 100%;
        max-width: 500px;
    }

    .highcharts-data-table caption {
        padding: 1em 0;
        font-size: 1.2em;
        color: #555;
    }

    .highcharts-data-table th {
        font-weight: 600;
        padding: 0.5em;
    }

    .highcharts-data-table td, .highcharts-data-table th, .highcharts-data-table caption {
        padding: 0.5em;
    }

    .highcharts-data-table thead tr, .highcharts-data-table tr:nth-child(even) {
        background: #f8f8f8;
    }

    .highcharts-data-table tr:hover {
        background: #f1f7ff;
    }
</style>
