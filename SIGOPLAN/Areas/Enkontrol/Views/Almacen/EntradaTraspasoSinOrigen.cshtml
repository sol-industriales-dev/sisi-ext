@{
ViewBag.Title = "EntradaTraspasoSinOrigen";
Layout = "~/Views/Shared/LayoutPage.cshtml";
}

<link href="~/Content/style/datatables/jquery.dataTables.css" rel="stylesheet" />
<link href="~/Content/style/datatables/dataTables.jqueryui.css" rel="stylesheet" />
<link href="~/Content/style/css/Enkontrol/Almacen/Movimientos.css" rel="stylesheet" />
<link href="~/Content/style/css/Style-Reportes.css" rel="stylesheet" />

<div class="container-fluid">
    <div class="row">
        <h2 class="text-center">Entrada Traspaso</h2>
    </div>

    <div class="row">
        <div class="col-xs-12 col-md-12 col-lg-12">
            <fieldset class="fieldset-custm" style="border-radius: 0px !important;">
                <div class="row">
                    <div class="col-xs-7 col-md-7 col-lg-7 noPadRight">
                        <fieldset class="fieldset-custm" style="border-radius: 0px !important;">
                            <legend class="legend-custm">DESTINO</legend>

                            <div class="col-xs-7 col-md-7 col-lg-7 noPadLeft noPadRight">
                                <div class="col-lg-12 mrgTop noPadLeft">
                                    <div class="input-group">
                                        <span class="input-group-addon">Almacén</span>
                                        <input id="inputAlmacenDestinoNum" class="form-control width30" tabindex="1">
                                        <input id="inputAlmacenDestinoDesc" class="form-control width70" disabled>
                                    </div>
                                </div>
                                <div class="col-lg-12 mrgTop noPadLeft">
                                    <div class="input-group">
                                        <span class="input-group-addon">Centro Costo</span>
                                        <input id="inputCCDestinoNum" class="form-control width30" tabindex="3">
                                        <input id="inputCCDestinoDesc" class="form-control width70" disabled>
                                    </div>
                                </div>
                                <div class="col-lg-12 mrgTop noPadLeft">
                                    <div class="input-group">
                                        <span class="input-group-addon">Recibió</span>
                                        <input id="inputRecibioNum" class="form-control width30" disabled>
                                        <input id="inputRecibioDesc" class="form-control width70" disabled>
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-5 col-md-5 col-lg-5 noPadLeft noPadRight">
                                <div class="col-lg-12 mrgTop noPadLeft noPadRight">
                                    <div class="input-group">
                                        <span class="input-group-addon">Número</span>
                                        <input id="inputNumero" class="form-control" tabindex="2">
                                    </div>
                                </div>
                                <div class="col-lg-12 mrgTop noPadLeft noPadRight">
                                    <div class="input-group">
                                        <span class="input-group-addon">Fecha</span>
                                        <input id="inputFecha" class="form-control" disabled>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-12 mrgTop noPadLeft noPadRight">
                                <div class="input-group">
                                    <span class="input-group-addon">Comentarios</span>
                                    <input id="inputComentarios" class="form-control" tabindex="5">
                                </div>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-xs-5 col-md-5 col-lg-5">
                        <fieldset class="fieldset-custm" style="border-radius: 0px !important;">
                            <legend class="legend-custm">ORIGEN</legend>

                            <div class="col-lg-12 mrgTop noPadLeft noPadRight">
                                <div class="input-group">
                                    <span class="input-group-addon">Almacén</span>
                                    <input id="inputAlmacenOrigenNum" class="form-control width30" tabindex="1">
                                    <input id="inputAlmacenOrigenDesc" class="form-control width70" disabled>
                                </div>
                            </div>
                            <div class="col-lg-12 mrgTop noPadLeft noPadRight">
                                <div class="input-group">
                                    <span class="input-group-addon">Centro Costo</span>
                                    <input id="inputCCOrigenNum" class="form-control width30" tabindex="3">
                                    <input id="inputCCOrigenDesc" class="form-control width70" disabled>
                                </div>
                            </div>
                            <div class="col-lg-12 mrgTop noPadLeft noPadRight">
                                <div class="input-group">
                                    <span class="input-group-addon">Orden Traspaso</span>
                                    <input id="inputOrdenTraspaso" class="form-control">
                                </div>
                            </div>
                            <div class="col-lg-7 mrgTop noPadLeft">
                                <div class="input-group">
                                    <span class="input-group-addon">Total</span>
                                    <input id="inputTotal" class="form-control text-right" disabled>
                                </div>
                            </div>
                            <div class="col-lg-5 mrgTop noPadLeft noPadRight">
                                <div class="input-group">
                                    <span class="input-group-addon">Folio Traspaso</span>
                                    <input id="inputFolioTraspaso" class="form-control">
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>

    <div class="row">
        <div class="col-xs-12 col-md-12 col-lg-12 mrgTop">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <button id="btnAgregarInsumo" type="button" class="btn btn-sm btn-primary"><i class="fa fa-plus"></i></button>
                    <button id="btnQuitarInsumo" type="button" class="btn btn-sm btn-danger"><i class="fa fa-minus"></i></button>
                    <button id="btnGuardar" class="btn btn-sm btn-primary pull-right"><i class="fa fa-save"> Guardar</i></button>

                    <fieldset class="mrgTop" style="border: 1px solid black;">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="widget">
                                    <div class="widget-header"></div>
                                    <div class="widget-content">
                                        <table id="tblPartidas" class="table-bordered hover" style="width: 100%;"></table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="mdlHistorialInsumo" role="dialog">
    <div class="modal-dialog modal-lg" role="document" style="width: 90%;">
        <div class="modal-content">
            <div class="modal-header modal-bg">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <div class="text-center">
                    <h3><label> Historial Insumo </label></h3>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="table-responsive col-md-12 col-lg-12 noPadLeft noPadRight">
                        <table id="tblHistorialInsumo" class="table table-fixed table-bordered marginBottom5" width="100%"></table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="mdlCatalogoUbicaciones" role="dialog">
    <div class="modal-dialog modal-lg" role="document" style="width: 90%;">
        <div class="modal-content">
            <div class="modal-header modal-bg">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <div class="text-center">
                    <h3><label> Catálogo Ubicaciones </label></h3>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="table-responsive col-md-12 col-lg-12 noPadLeft noPadRight">
                        <table id="tblCatalogoUbicaciones" class="table table-fixed table-bordered marginBottom5" width="100%"></table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/Utilities/datatables/jquery.dataTables.js"></script>
<script src="~/Scripts/Utilities/datatables/dataTables.jqueryui.js"></script>
<script src="~/Scripts/Utilities/datatables/datatables.min.js"></script>
<script src="~/Scripts/js/Enkontrol/Almacen/EntradaTraspasoSinOrigen.js"></script>

@Html.Partial("reportViewerView", false)