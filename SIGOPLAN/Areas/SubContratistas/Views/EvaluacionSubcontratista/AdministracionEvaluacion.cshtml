@{
    Layout = "~/Views/Shared/LayoutPage.cshtml";
}

<link href='~/Content/style/datatables/datatables.min.css' rel='stylesheet' />
<link href="~/Content/select2.css" rel="stylesheet" />
<link href="~/Content/style/starrr.css" rel="stylesheet" />
<link href="~/Content/style/css/Style-Reportes.css" rel="stylesheet" />
<link href="~/Content/style/css/Subcontratistas/EstiloGeneral.css" rel="stylesheet" />

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12 text-center">
            <br>
            <h2>ADMINISTRACIÓN DE EVALUACIONES SUBCONTRATISTAS</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-default panel-principal panelFiltro">
                <div class="panel-heading">
                    <h5 class="text-center">FILTRO DE BÚSQUEDA</h5>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-sm-3">
                            <div class="input-group">
                                <span class="input-group-addon botonNaranja">Estatus</span>
                                <select id="selectEstatus" class="form-control">
                                    <option value="">--Seleccione--</option>
                                    <option value="1">PENDIENTES POR ASIGNAR</option>
                                    <option value="2">EVALUACIÓN ASIGNADA</option>
                                    <option value="3">ESTATUS DE EVALUACIÓN</option>
                                    <option value="4">COMPROMISOS</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <div class="input-group">
                                <span class="input-group-addon">CC</span>
                                <select id="selectProyecto" class="form-control select2"></select>
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <div class="input-group">
                                <span class="input-group-addon">Subcontratista</span>
                                <select id="selectSubcontratista" class="form-control select2"></select>
                            </div>
                        </div>

                        <div class="col-sm-1">
                            <button id="botonBuscar" class="btn btn-primary"><i class="fa fa-search"></i>&nbsp;Buscar</button>
                        </div>
                    </div>
                </div>
            </div>

            <div id="divGeneral">
                <div class="panel panel-default panel-principal panelFiltro">
                    <div class="panel-heading">
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-2"></div>
                            <div class="col-sm-2" style="text-align: center;">
                                <label style="display: block;">Pendientes por asignar</label>
                                <input id="inputPendientesAsignar" class="form-control text-center inputIndicador" disabled>
                            </div>

                            <div class="col-sm-2" style="text-align: center;">
                                <label style="display: block;">Asignadas</label>
                                <input id="inputAsignadas" class="form-control text-center inputIndicador" disabled>
                            </div>

                            <div class="col-sm-2" style="text-align: center;">
                                <label style="display: block;">Autorizadas</label>
                                <input id="inputAutorizadas" class="form-control text-center inputIndicador" disabled>
                            </div>

                            <div class="col-sm-2" style="text-align: center;">
                                <label style="display: block;">Cumplimiento</label>
                                <input id="inputCumplimiento" class="form-control text-center inputIndicador" disabled>
                            </div>
                            <div class="col-sm-2"></div>
                        </div>
                    </div>
                </div>

                <div class="panel panel-default panel-principal panelFiltro">
                    <div class="panel-heading">
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <table id="tablaEvaluaciones" class="table compact" style="width: 100%"></table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="divCompromisos" style="display: none;">
                <div class="panel panel-default panel-principal panelFiltro">
                    <div class="panel-heading">
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="col-sm-12">
                                    <label style="display: block; text-align: center;">Requerimientos Evaluados</label>
                                </div>
                                <div class="col-sm-12">
                                    <table id="tablaRequerimientosEvaluados" class="table compact dataTable" style="width: 100%">
                                        <thead>
                                            <tr>
                                                <th>Total Requerimientos Evaluados</th>
                                                <th>Requerimientos Aprobados</th>
                                                <th>Requerimientos No Aprobados</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td id="celdaTotalRequerimientosEvaluados" class="text-center"></td>
                                                <td id="celdaRequerimientosAprobados" class="text-center"></td>
                                                <td id="celdaRequerimientosNoAprobados" class="text-center"></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="col-sm-12">
                                    <label style="display: block; text-align: center;">Compromisos Por Evaluación</label>
                                </div>
                                <div class="col-sm-12">
                                    <table id="tablaCompromisosPorEvaluacion" class="table compact dataTable" style="width: 100%">
                                        <thead>
                                            <tr>
                                                <th>Total De Compromisos</th>
                                                <th>Promedio Cumplimiento</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td id="celdaTotalCompromisos" class="text-center"></td>
                                                <td id="celdaPromedioCumplimiento" class="text-center"></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="panel panel-default panel-principal panelFiltro">
                    <div class="panel-heading">
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <table id="tablaCompromisos" class="table compact" style="width: 100%"></table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id='modalAsignacion' class='modal fade' role='dialog'>
    <div class='modal-dialog modal-lg'>
        <div class='modal-content'>
            <div class='modal-header text-center'>
                <h4 class='modal-title'>Asignar Evaluación</h4>
            </div>
            <div class='modal-body'>
                <div class="row">
                    <div class='col-sm-12 mrgTop'>
                        <fieldset class="fieldset-custm fieldsetNaranja text-center">
                            <legend class="legend-custm" style="border-radius: 5px; font-size: 14px; font-weight: bold;">Fecha de Ejecución del Proyecto</legend>
                            <div class="row">
                                <div class="col-sm-6 text-center">
                                    <label>Fecha Inicial</label>
                                    <input id="inputFechaInicial" class="form-control text-center" autocomplete="off">
                                </div>
                                <div class="col-sm-6 text-center">
                                    <label>Fecha Final</label>
                                    <input id="inputFechaFinal" class="form-control text-center" autocomplete="off">
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>
                <div class="row">
                    <div class='col-sm-12 mrgTop'>
                        <fieldset class="fieldset-custm fieldsetNaranja">
                            <div class="row">
                                <div class="col-sm-12 text-center">
                                    <label>Especialidad (Servicio Contratado)</label>
                                    <input id="inputEspecialidad" class="form-control text-center" disabled>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6 text-center">
                                    <label>Estado</label>
                                    <input id="inputEstado" class="form-control text-center" disabled>
                                </div>
                                <div class="col-sm-6 text-center">
                                    <label>Municipio</label>
                                    <input id="inputMunicipio" class="form-control text-center" disabled>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6 text-center">
                                    <label>Evaluación Inicial</label>
                                    <input id="inputEvaluacionInicial" class="form-control text-center" disabled>
                                </div>
                                <div class="col-sm-6 text-center">
                                    <label>Evaluación Final</label>
                                    <input id="inputEvaluacionFinal" class="form-control text-center" disabled>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12 text-center">
                                    <label>Evaluaciones Requeridas (Periódicas)</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div id="divEvaluaciones" class="col-sm-12 text-center" style="min-height: 150px; max-height: 150px; overflow: auto; background-color: white !important;">

                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>
            </div>
            <div class='modal-footer'>
                <button type='button' class='btn btn-danger' data-dismiss='modal'><i class='fa fa-times'></i>&nbsp;Cancelar</button>
                <button id="botonGuardarAsignacion" type="button" class="btn btn-success"><i class='fa fa-save'></i>&nbsp;Confirmar</button>
            </div>
        </div>
    </div>
</div>

<div id='modalActualizarEvaluacion' class='modal fade' role='dialog'>
    <div class='modal-dialog modal-lg'>
        <div class='modal-content'>
            <div class='modal-header text-center'>
                <h4 class='modal-title'>Actualizar Evaluación</h4>
            </div>
            <div class='modal-body'>
                <div class="row">
                    <div class='col-sm-12 mrgTop'>
                        <label>Si desea cambiar la fecha de evaluación indique y notifique al administrador.</label>
                    </div>
                </div>
                <div class="row">
                    <div class='col-sm-6 mrgTop'>
                        <label>Fecha de Evaluación</label>
                        <input id="inputFechaEvaluacionActualizar" class="form-control text-center" autocomplete="off">
                    </div>
                    <div class='col-sm-6 mrgTop'>
                        <label>Solicitante</label>
                        <input id="inputSolicitante" class="form-control">
                    </div>
                    <div class='col-sm-12 mrgTop'>
                        <label>Motivo de Cambio</label>
                        <textarea id="textareaMotivo" class="form-control" rows="3"></textarea>
                    </div>
                </div>
            </div>
            <div class='modal-footer'>
                <button id="botonNotificarActualizacionEvaluacion" type="button" class="btn botonNaranja">Notificar</button>
            </div>
        </div>
    </div>
</div>

<div id='modalAutorizarCambioEvaluacion' class='modal fade' role='dialog'>
    <div class='modal-dialog modal-lg'>
        <div class='modal-content'>
            <div class='modal-header text-center'>
                <h4 class='modal-title'>Autorizar Cambio Evaluación</h4>
            </div>
            <div class='modal-body'>
                <div class="row">
                    <div class='col-sm-12 mrgTop'>
                        <label>Si desea cambiar la fecha de evaluación indique y notifique al administrador.</label>
                    </div>
                </div>
                <div class="row">
                    <div class='col-sm-6 mrgTop'>
                        <label>Fecha de Evaluación</label>
                        <input id="inputFechaEvaluacionAutorizar" class="form-control text-center" autocomplete="off" disabled>
                    </div>
                    <div class='col-sm-6 mrgTop'>
                        <label>Solicitante</label>
                        <input id="inputSolicitanteAutorizar" class="form-control" disabled>
                    </div>
                    <div class='col-sm-12 mrgTop'>
                        <label>Motivo de Cambio</label>
                        <textarea id="textareaMotivoAutorizar" class="form-control" rows="3" disabled></textarea>
                    </div>
                </div>
                <div class="row">
                    <div class='col-sm-12 mrgTop'>
                        <label>Comentario Autorizante</label>
                        <textarea id="textareaComentarioAutorizante" class="form-control" rows="3"></textarea>
                    </div>
                </div>
            </div>
            <div class='modal-footer'>
                <button id="botonRechazarCambioEvaluacion" type="button" class="btn btn-danger" estatus="3">Rechazar</button>
                <button id="botonAutorizarCambioEvaluacion" type="button" class="btn btn-success" estatus="2">Aprobar</button>
            </div>
        </div>
    </div>
</div>

<div id='modalSoporte' class='modal fade' role='dialog'>
    <div class='modal-dialog modal-lg' style="width: 95%;">
