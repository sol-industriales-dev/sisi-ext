@{
    ViewBag.Title = "reporte Ejecutivo";
    Layout = "~/Views/Shared/LayoutPage.cshtml";
}

<link href="~/Content/style/css/Style-Reportes.css" rel="stylesheet" />
<script src="~/Scripts/js/Maquinaria/Barrenacion/ReporteEjecutivo.js"></script>

<div class="row">
    <div class="col-lg-12">
        <h3 class="text-center">REPORTE </h3>
    </div>
</div>
<div class="row">
    <div class="col-md-3 col-lg-3">
        <div class="input-group">
            <span class="input-group-addon">Area Cuenta</span>
            <select name="" id="comboAC" class="form-control" multiple></select>
        </div>
    </div>
    <div class="col-xs-12 col-md-4 col-lg-4">
        <div class="input-group">
            <span class="input-group-addon">Perforadora:</span>
            <select id="comboBarrenadora" class="form-control" multiple></select>
        </div>
    </div>
    <div class="col-md-2 col-lg-2">
        <div class="input-group">
            <span class="input-group-addon">Fecha inicio:</span>
            <input class="text-center form-control" type="text" id="inputFechaInicio">
        </div>
    </div>

    <div class="col-md-2 col-lg-2">
        <div class="input-group">
            <span class="input-group-addon">Fecha fin:</span>
            <input class="text-center form-control" type="text" id="inputFechaFin">
        </div>
    </div>
    <div class="col-lg-2">
        <button id="btnBuscar" class="btn btn-primary">
            <i class="fas fa-search"></i> Buscar
        </button>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <fieldset class="fieldset-custm">
            <legend class="legend-custm">Vista Preliminar:</legend>
            <div class="clsFrame text-center form-group">
                <iframe id="report" align="middle" marginwidth="0"
                        marginheight="0"
                        hspace="0"
                        vspace="0"
                        frameborder="0"
                        scrolling="no" class="StyleIframe"></iframe>

            </div>
        </fieldset>
    </div>
</div>
