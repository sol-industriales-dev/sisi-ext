@{
    ViewBag.Title = "TransferenciasBancarias";
    Layout = "~/Views/Shared/LayoutPage.cshtml";
}

<link href="~/Content/style/datatables/jquery.dataTables.css" rel="stylesheet" />
<link href='~/Content/select2.css' rel='stylesheet' />
<link href="~/Content/style/css/Style-Reportes.css" rel="stylesheet" />
<link href="~/Content/style/css/Maquinaria/Rentabilidad/Externos/rowGroup.dataTables.min.css" rel="stylesheet" />

<style>
    #tablaIntereses_wrapper thead tr th {
        background: #81bd72 !important;
        color: #f5f5f5;
    }

    .mrgTop {
        margin-top: 5px;
    }
</style>

<div class="row text-center margin-top">
    <h2>Reporte de Intereses Pagados</h2>
</div>

<fieldset class="fieldset-custm">
    <legend class="legend-custm">Filtros:</legend>
    <div class="row">
        <div class="col-md-2">
            <label>Fecha Corte</label>
            <input id="inputFechaCorte" type="text" class="form-control">
        </div>
        <div class="col-md-2">
            <button type="button" class="btn btn-primary" id="botonBuscar" style="margin-top: 25px;"><i class="fas fa-search"></i> Buscar</button>
        </div>
    </div>
</fieldset>

<fieldset class="fieldset-custm margin-top">
    <legend class="legend-custm"></legend>
    <div class="row">
        <div class="col-sm-12 margin-top">
            <button id="btnExcel" class="btn btn-info"><i class="fas fa-file-download"></i> EXCEL</button>
        </div>

        <div class="col-xs-12 mrgTop">
            <table id="tablaIntereses" class="table compact" style="width:100%;">
                <tfoot>
                    <tr>
                        <th colspan="8">TOTAL</th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
</fieldset>

