@{
    ViewBag.Title = "Disponibilidad";    
}

<script src="~/Scripts/js/Principal/Home/Disponibilidad/Disponibilidad.js"></script>

<br />
<div style="text-align:center">
    <h1>Alertas Disponibilidad</h1>
</div>
<div class="table100 ver6 m-b-110">
    <table class="animated pulse table-responsive-data2 table-striped table-bordered" id="contenido">
        <thead>
            <tr id="contenidoTabla">
                <th data-column-id="maquinaID">Económico</th>
                <th data-column-id="descripcion">Descripción</th>
                <th data-column-id="cc" data-formatter="CCName">Obra</th>
                <th data-column-id="decimalDisponibilidad" data-formatter="Disponibilidad" data-type="numeric">Disponibilidad</th>          
                <th data-column-id="fecha" >Fecha</th>      
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>
