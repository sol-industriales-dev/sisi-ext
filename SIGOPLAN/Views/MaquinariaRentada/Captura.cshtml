<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <h2 class="text-center">CAPTURA RENTA DE MAQUINARIA</h2>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-default">
                <div class="panel-heading"><h3>Control de renta de maquinaria</h3></div>
                <div class="panel-body">
                    <!-- Filtros tabla maquinas rentadas -->
                    <div class="row">
                        <div class="col-sm-3">
                            <div class="input-group">
                                <span class="input-group-addon">Área cuenta</span>
                                <select class="form-control" id="cboFiltroAreaCuenta" value=""></select>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="input-group">
                                <span class="input-group-addon">Centro de costo</span>
                                <select class="form-control" id="cboFiltroCentroCosto" value=""></select>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="input-group">
                                <span class="input-group-addon">Periodo desde</span>
                                <input type="text" class="form-control" id="txtFiltroPeriodoDesde" />
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="input-group">
                                <span class="input-group-addon">Periodo hasta</span>
                                <input type="text" class="form-control" id="txtFiltroPeriodoHasta" />
