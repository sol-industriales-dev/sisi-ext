                            <button type="button" class="btn btn-primary pull-right" id="btnReporteTiempoUtilizadoVs" value="199">Reporte</button>
                        </div>
                    </div>
                </div>
                <!-- Tabla maquinas rentadas -->
                <div class="row">
                    <div class="col-sm-12">
                        <div class="table-responsive">
                            <table id="tblMaquinasRentadas" class="table cell-border hover" style="width:100%"></table>
                        </div>
                    </div>
                </div>
                <div class="panel-footer"></div>
            </div>
        </div>
    </div>
</div>

<!-- Modal opciones de la tabla maquinas rentadas -->
<div id="modalOpcionesTblMaquinasRentadas" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header modal-bg">
                <button type="button" class="close" data-dismiss="modal" aria-label="Cerrar"><span aria-hidden="true">&times;</span></button>
                <div class="text-center">
                    <h3 class="modal-title">Opciones</h3>
                </div>
            </div>
            <div class="modal-body text-center"></div>
        </div>
    </div>
</div>

