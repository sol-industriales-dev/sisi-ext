                                    <input class="form-control" id="txtDescripcionActividad" type="text" />
                                </div>
                                <div class="col-xs-5 col-sm-5 col-md-5 col-lg-5">
                                    <label>Tipo:</label>
                                    <select class="form-control" id="cboTipoActividad" title="Seleccione:"></select>
                                </div>
                                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                    <button type="button" class="btn btn-info margin-top" id="btnBuscar_Actividad">
                                        <span class="glyphicon glyphicon-search"></span> Buscar
                                    </button>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-2 col-lg-2 text-center">
                        <fieldset class="fieldset-custm">
                            <legend class="legend-custm">Actividad:</legend>
                            <button type="button" class="btn btn-success margin-top" id="btnNueva_Actividad">
                                <span class="glyphicon glyphicon-plus"></span> Nuevo
                            </button>
                        </fieldset>
                    </div>

                </div>

                <div class="row form-group">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" id="divFilstroBusqueda">
                        <fieldset class="fieldset-custm">
                            <legend class="legend-custm">Resultados Búsqueda:</legend>
                            <table id="grid_Actividad" class="table table-condensed table-hover table-striped text-center">
                                <thead class="bg-table-header">
                                    <tr>
                                        <th data-column-id="descripcionActividad" data-header-align="center">Descripción</th>
                                        <th data-column-id="TipoActividad" data-header-align="center">Tipo Actividad</th>
                                        <th data-column-id="update" data-formatter="update" data-align="center" data-header-align="center" data-sortable="false" data-width="6%">Modificar</th>
                                        <th data-column-id="delete" data-formatter="delete" data-align="center" data-header-align="center" data-sortable="false" data-width="6%">Eliminar</th>
                                    </tr>
                                </thead>

                            </table>

                        </fieldset>
                    </div>
                </div>


            </div>
        </div>
        <div class="modal-footer modal-footerg">
            <a data-dismiss="modal" class="btn btn-primary btn-sm"><span class="glyphicon glyphicon-remove"></span>Cerrar</a>
        </div>
    </div>
</div>

