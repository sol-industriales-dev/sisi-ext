@*Carga de actividades*@
<div class="modal fade  modalg" id="modalActividad" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static">
    <div class="modal-dialog modal-lg modal-dialogg" role="document">
        <div class="modal-content modal-contentg">
            <div class="modal-header modal-bg modal-headerg">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <div class="text-center">
                </div>
            </div>
            <div class="modal-body modal-bodyg container-fluid CuerpoModalDet" style="text-align:center">
                <div class="row">
                    <h2 class="text-center">Catálogo de Actividades</h2>
                </div>
                <div class="row form-group">
                    <div class="col-lg-10">
                        <fieldset class="fieldset-custm fielset-Filtros">
                            <legend class="legend-custm" style="text-align:left">Busqueda:</legend>

                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="text-align:left">
                                <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
