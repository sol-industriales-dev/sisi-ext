        <li><a id="tabRemociones" data-toggle="tab" href="#remociones">Remociones</a></li>
        <li><a id="tabCRC" data-toggle="tab" href="#crc">CRC</a></li>
        <li><a id="tabAlmacen" data-toggle="tab" href="#almacen">Almacén</a></li>
        <li><a id="tabInactivos" data-toggle="tab" href="#inactivos">Inactivos</a></li>
    </ul>
    <div class="tab-content">
        <div id="inventario" class="tab-pane">
            <div class="row form-group">
                <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10 text-center">
                    <fieldset class="fieldset-custm fielset-Filtros">
                        <legend class="legend-custm">Filtros</legend>
                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-3">
                            <label>Serie Componente:</label>
                            <input class="form-control" id="txtFiltroNoComponente" type="text" maxlength="100">
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-3">
                            <label>Conjunto:</label>
                            <select class="form-control" id="cboConjunto" style="width: 100%;"></select>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-3">
                            <label>Subconjunto:</label>
                            <select class="form-control" id="cboSubconjunto" style="width: 100%;"></select>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-3">
