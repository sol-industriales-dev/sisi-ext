<h2 class="text-center">Taller Overhaul</h2>
<div class="col-xs-12 col-sm-12 col-md-2 col-lg-2 no-padding">
    <fieldset class="fieldset-custm fielset-Filtros no-padding">
        <legend class="legend-custm">Filtros:</legend>
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="padding: 0 8px 0 8px;">
            <label>Calendario:</label>
            <select class="form-control" id="cboCalendarioEstatus"></select>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="padding: 0 8px 0 8px;">
            <label>Tipo:</label>
            <select class="form-control" id="cboTipoParo">
                <option selected>Todos</option>
                <option value="0">Programado</option>
                <option value="3">Falla</option>
            </select>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="padding: 0 8px 0 8px;">
            <label>Estatus:</label>
            <select class="form-control" id="cboEstatusTaller">
                <option value="0">Sin ejecutar</option>
                <option value="1" selected>En taller</option>
                <option value="2">Terminado</option>
            </select>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 text-center" style="padding: 10px 8px 10px 8px;">
            <button type="button" class="btn btn-info" id="btnBuscarEstatus">
