		<add key="DoconutPngExportPath" value="~/Export"/>
		<add key="DoconutUnSafeMode" value="false"/>
		<add key="DoconutShowInfo" value="true"/>
		<add key="DoconutPageWaitTimeSeconds" value="0"/>
		<add key="DoconutStartWaitFromPage" value="5"/>
	</appSettings>-->
</configuration>